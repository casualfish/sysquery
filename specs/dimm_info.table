    Column("model", TEXT, "server model"),
    Column("sn", TEXT, "dimm sn"),
    Column("slot", TEXT, "dimm slot"),
    Column("capacity", INTEGER, "dimm capacity"),
    Column("width", INTEGER, "dimm width"),
])
implementation("system@genDIMMInfo")
